- document = document.nil? ? index : document

- if params[:controller] == "catalog"
  =render :partial=>'recordings/table_result_for_catalog', :locals => {:document => document}
- else
  -# if ActiveFedora::ContentModel.known_models_for(part).include?(S3Content)
  - if document["has_model_s"].include?("info:fedora/afmodel:S3Audio")
    =render :partial=>'s3_audio/index', :locals => {:document => document}
  - if document["has_model_s"].include?("info:fedora/afmodel:S3Video")
    =render :partial=>'s3_video/index', :locals => {:document => document}
  - elsif document["has_model_s"].include?("info:fedora/afmodel:S3Content")
    =render :partial=>'s3_contents/index', :locals => {:document => document}
  - else
    =render :partial=>'recordings/table_result', :locals => {:document => document}

  


