%tr[document]
  %td
    =link_to document["title_t"], catalog_path(document["id"])
  %td
    =document["date_t"]
  %td
    =document["location_t"]
  %td
    =render :partial=>"shared/solr_result_field_value", :collection => document["subject_t"]
  %td
    = document[:has_collection_member_s] ? document[:has_collection_member_s].length : 0
